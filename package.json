{
  "name": "@bassnotion-monorepo-v1/source",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "NX_CLOUD_ACCESS_TOKEN= npx nx build @bassnotion/backend --prod --skip-nx-cache --verbose --force",
    "start": "node dist/apps/backend/main.js",
    "build:backend": "NX_CLOUD_ACCESS_TOKEN= npx nx build @bassnotion/backend --prod --skip-nx-cache --verbose --force",
    "start:backend": "node dist/apps/backend/main.js",
    "build:frontend": "npx nx build @bassnotion/contracts && npx nx build @bassnotion/frontend",
    "dev:backend": "npx nx serve @bassnotion/backend",
    "dev:frontend": "npx nx serve @bassnotion/frontend",
    "dev": "npx nx run-many --target=serve --projects=@bassnotion/backend,@bassnotion/frontend --parallel=true",
    "deps:check": "./scripts/dependency-update.sh",
    "deps:audit": "pnpm audit --audit-level moderate",
    "deps:outdated": "pnpm outdated",
    "deps:safe": "pnpm audit --fix && pnpm update --save-exact"
  },
  "private": true,
